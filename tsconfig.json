{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "lib": ["ESNext", "DOM"],
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
